		position: relative
		padding: 20px
		float: left
		width: 660px
		margin: 20px 0px
		h2
			top: -23px
			padding: 5px
			position: absolute
			background: white
			color: $rosa
			font-family: $roboto
			font-size: 30px
			font-weight: 300

		.post
			border-bottom: 2px solid #d9d9d9
			padding: 15px 0px

			h3
				font-family: $roboto
				color: $rosa
				text-transform: uppercase
				font-size: 19px

			img
				margin: 10px 0px
				float: left
				margin-right: 10px

			p
				margin-top: 10px
				color: $cinza
				font-size: 14px
				line-height: 1.4

			a.continuar
				background: $cinza
				color: white
				padding: 10px
				display: block
				width: 200px
				text-align: center
				float: right
				border: 1px solid $cinza
				+transition(0.3s ease-out)

				&:hover
					background: transparent
					color: $cinza

			&:last-of-type
				border-bottom: none 

	aside
